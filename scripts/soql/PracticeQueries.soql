SELECT Id, Name, Status, Disqualified_Reason__c, Location_Interest__c, ConvertedOpportunity.StageName
FROM Lead WHERE (Status IN ('Disqualified') AND Disqualified_Reason__c IN ('Unresponsive'))
OR (Location_Interest__c LIKE 'Brooklyn%' AND ConvertedOpportunity.StageName IN ('Cancelled')) OR (ConvertedOpportunity.StageName IN ('Seen'))